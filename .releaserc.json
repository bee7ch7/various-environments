{
   "branches":[
      "main",
      "dev",
      {
         "name":"stage",
         "prerelease":true
      }
   ],
   "plugins":[
      [
         "@semantic-release/commit-analyzer",
         {
            "preset":"angular",
            "releaseRules":[
               {
                  "type":"docs",
                  "scope":"README",
                  "release":"patch"
               },
               {
                  "type":"bug",
                  "release":"patch"
               },
               {
                  "type":"fix",
                  "release":"patch"
               },
               {
                  "type":"refactor",
                  "release":"patch"
               },
               {
                  "type":"feature",
                  "release":"minor"
               },
               {
                  "type":"release",
                  "release":"major"
               },
               {
                  "scope":"no-release",
                  "release":false
               }
            ]
         }
      ],
      "@semantic-release/release-notes-generator",
      [
         "@saithodev/semantic-release-backmerge",
         {
            "backmergeBranches": ["dev"],
            "message": "docs: Preparations for next release [skip ci]"
         }
      ]
   ]
}
