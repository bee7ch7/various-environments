{
   "branches":[
      "main",
      {
         "name":"dev",
         "prerelease":true
      },
      {
         "name":"stage",
         "prerelease":true
      }
   ],
   "plugins":[
      [
         "@semantic-release/exec",
         {
            "successCmd":"if \[ $(git rev-parse --abbrev-ref HEAD) == 'main' \]; then gh pr create --base dev --head main --title 'docs: [skip ci] backmerge' --body '[skip ci] backmerge' ; fi"
         }
      ],
      [
         "@semantic-release/commit-analyzer",
         {
            "preset":"angular",
            "releaseRules":[
               {
                  "type":"docs",
                  "scope":"README",
                  "release":"patch"
               },
               {
                  "type":"bug",
                  "release":"patch"
               },
               {
                  "type":"fix",
                  "release":"patch"
               },
               {
                  "type":"refactor",
                  "release":"patch"
               },
               {
                  "type":"feature",
                  "release":"minor"
               },
               {
                  "type":"release",
                  "release":"major"
               },
               {
                  "scope":"no-release",
                  "release":false
               }
            ]
         }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
   ]
}
